#include<stdio.h>
#include<math.h>
#define pi 3.14159
int main()
{
    double A,B,C,T,Ci,Tra,Sq,Rec;
    scanf("%lf %lf %lf",&A,&B,&C);
    T=1.0/2*(A*C);
    Ci=pi*pow(C,2);
    Tra=1.0/2*(A+B)*C;
    Sq=pow(B,2);
    Rec=(A*B);
    printf("TRIANGULO: %.3lf\n",T);
    printf("CIRCULO: %.3lf\n",Ci);
    printf("TRAPEZIO: %.3lf\n",Tra);
    printf("QUADRADO: %.3lf\n",Sq);
    printf("RETANGULO: %.3lf\n",Rec);
    return 0;
}
