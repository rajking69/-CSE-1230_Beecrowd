#include<stdio.h>
int main()
{
    int min,mid,max,a,b,c,temp;
    scanf("%d %d %d",&min,&mid,&max);
    a=min;
    b=mid;
    c=max;
    if(a>b) 
    {
        temp=a;
        a=b;
        b=temp; 
    }
    if(a>c)
    {
        temp=a;
        a=c;
        c=temp;
    }
    if(b>c)
    {
        temp=b;
        b=c;
        c=temp;
    }

    printf("%d\n%d\n%d\n\n",a,b,c);
    printf("%d\n%d\n%d\n",min,mid,max);
    return 0;
}
